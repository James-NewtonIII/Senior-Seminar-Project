<p id="notice"><%= notice %></p>

<h1>Tafs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tafs.each do |taf| %>
      <tr>
        <td><%= link_to 'Show', taf %></td>
        <td><%= link_to 'Edit', edit_taf_path(taf) %></td>
        <td><%= link_to 'Destroy', taf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taf', new_taf_path %>
